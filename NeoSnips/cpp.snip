# for
snippet for_int
alias for
abbr for ( int i )
options head
    for ( int ${1:i} = 0; $1 <  ${2}; $1++ )
    {
        ${0}
    }

# for each
snippet for_each
abbr for ( i : thing )
options head
    for ( ${1:auto} ${2:var} : ${3:collection} )
    {
        ${0}
    }

